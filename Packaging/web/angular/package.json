  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disableHostCheck=true --host=0.0.0.0",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build",
    "test": "npm run lint && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --browsers ChromeHeadless --watch",
    "build:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "clean": "rimraf ./dist/",
    "server": "cd dist && http-server",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --browsers ChromeTravisCi && ng e2e && npm run build:prod -- --deploy-url /angular-ngrx-material-starter/ --base-href /angular-ngrx-material-starter",
    "release": "standard-version && git push --follow-tags origin master",
    "prettier": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "prettier:ci": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "build:bugfix": "npm install --unsafe-perm && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=bugfix",
    "build:dev": "npm install --unsafe-perm && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=dev",
    "build:master": "npm install --unsafe-perm && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=master",
    "build:kuber_dev": "npm install --unsafe-perm && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=kuber_dev",
    "build:kuber_prod": "npm install --unsafe-perm && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=kuber_prod",
    "start:local": "ng serve --disableHostCheck=true --host=0.0.0.0 --configuration=local",
    "start:bugfix": "cd dist && http-server --cors --port=3000",
    "start:dev": "cd dist && http-server --cors --port=3002",
    "start:master": "cd dist && http-server --cors --port=3004"
  },
